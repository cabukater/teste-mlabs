it('should delete an item', () => {
    const likert = { id: 1, name: 'Test', value: 1 };
    spyOn(component.likertJService, 'delete').and.returnValue(of(true));
    spyOn(component, 'alertModal');
    spyOn(component.likertList$, 'subscribe');
    component.DeleteItem(likert);
    expect(component.likertJService.delete).toHaveBeenCalledWith(likert.id);
    expect(component.alertModal).toHaveBeenCalledWith('', 'success');
    expect(component.likertList$.subscribe).toHaveBeenCalled();
  });
